{
  "name": "emotionalchain-poe-blockchain",
  "version": "1.0.0",
  "description": "EmotionalChain - The world's first emotion-based blockchain consensus platform using Proof of Emotion (PoE) with custom blockchain implementation",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "blockchain",
    "emotion",
    "biometric",
    "consensus",
    "proof-of-emotion",
    "wellness",
    "ai",
    "custom-blockchain",
    "typescript",
    "p2p-network"
  ],
  "author": "EmotionalChain Team",
  "homepage": "https://emotionalchain.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChronoCoders/Proof-of-Emotion"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:windows": "set NODE_ENV=development && tsx server/index.ts",
    "dev:unix": "NODE_ENV=development tsx server/index.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:windows": "set NODE_ENV=production && node dist/index.js",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:windows": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "check": "tsc",
    "check:watch": "tsc --watch",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo \"ESLint configuration - implement when needed\" && exit 0",
    "consensus:test": "cross-env NODE_ENV=test tsx server/consensus/test-runner.ts",
    "validators:check": "cross-env NODE_ENV=development tsx server/validators/health-check.ts",
    "biometrics:test": "cross-env NODE_ENV=test tsx server/biometrics/integration-test.ts",
    "network:stats": "cross-env NODE_ENV=development tsx server/analytics/network-stats.ts",
    "export:data": "cross-env NODE_ENV=development tsx server/export/data-export.ts",
    "install:deps": "npm install",
    "setup": "npm run install:deps && npm run check && echo \"EmotionalChain setup complete!\"",
    "demo": "echo \"Starting EmotionalChain Demo...\" && npm run dev",
    "production": "npm run build && npm run start",
    "ml:setup": "python setup_ml_engine.py",
    "ml:train": "python ml-engine/train_models.py",
    "ml:test": "python -c \"import sys; sys.path.append('ml-engine'); from emotion_classifier import EmotionClassifier; print('ML Engine OK')\"",
    "ml:generate-data": "python ml-engine/training_data/generate_data.py",
    "blockchain:build": "tsc -p emotional-chain/ --outDir dist/blockchain",
    "blockchain:start": "node dist/blockchain/main.js",
    "blockchain:dev": "tsx emotional-chain/SRC/main.ts",
    "blockchain:wallet": "tsx emotional-chain/SRC/wallet-cli.ts",
    "blockchain:test": "jest emotional-chain/",
    "blockchain:clean": "rm -rf dist/blockchain",
    "custom-chain:start": "npm run blockchain:dev",
    "custom-chain:wallet": "npm run blockchain:wallet",
    "custom-chain:build": "npm run blockchain:build && npm run blockchain:start"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-aspect-ratio": "^1.1.3",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-collapsible": "^1.1.4",
    "@radix-ui/react-context-menu": "^2.2.7",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-hover-card": "^1.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-menubar": "^1.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.6",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slider": "^1.2.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.7",
    "@radix-ui/react-toggle": "^1.1.3",
    "@radix-ui/react-toggle-group": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@tanstack/react-query": "^5.60.5",
    "better-sqlite3": "^12.2.0",
    "browserslist": "^4.25.1",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "connect-pg-simple": "^10.0.0",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "embla-carousel-react": "^8.6.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "fix": "^0.0.3",
    "framer-motion": "^11.13.1",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.453.0",
    "memorystore": "^1.6.7",
    "nanoid": "^5.1.5",
    "next-themes": "^0.4.6",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.4.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.5",
    "vaul": "^1.1.2",
    "wouter": "^3.3.5",
    "ws": "^8.18.3",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "readline": "^1.3.0",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/better-sqlite3": "^7.6.13",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express": "4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/node": "20.16.11",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.1",
    "typescript": "5.6.3",
    "vite": "^6.3.5",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "@types/cors": "^2.8.16"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "config": {
    "emotionalchain": {
      "consensus": "proof-of-emotion",
      "version": "1.0.0",
      "network": {
        "minValidators": 3,
        "consensusThreshold": 67,
        "blockTime": 2000,
        "defaultPort": 8001,
        "bootstrapNodes": ["localhost:8000"],
        "maxPeers": 50
      },
      "blockchain": {
        "genesisTimestamp": 1640995200000,
        "difficultyAdjustmentInterval": 2016,
        "initialDifficulty": 4,
        "miningReward": 10,
        "minTransactionFee": 0.001
      },
      "validator": {
        "minStake": 1000,
        "emotionalConsistencyThreshold": 0.75,
        "uptimeThreshold": 0.90,
        "biometricAuthenticityThreshold": 0.75
      },
      "biometric": {
        "dataEncryption": true,
        "enableTestBiometrics": true,
        "mockBiometricDevices": true,
        "sensors": ["heartRate", "facial", "voice", "skin", "eyeTracking"]
      }
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/emotional-chain", "<rootDir>/tests"],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/*.(test|spec).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "collectCoverageFrom": [
      "emotional-chain/**/*.{ts,tsx}",
      "!emotional-chain/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "testTimeout": 10000
  }
}